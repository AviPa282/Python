 digit: \"))\n","    self.tens = int(input(\"Enter the tens digit: \"))\n","    self.hundreds = int(input(\"Enter the hundreds digit: \"))\n","  \n","  def findnumber(self):\n","    totalnum = self.units + self.tens*10 + self.hundreds*100\n","    return totalnum\n","  \n","  def FindArmstrong(self):\n","    totalcube = self.units**3 + self.tens**3 + self.hundreds**3\n","    if totalcube == self.findnumber():\n","      print(\"The number is armstrong.\")\n","    else:\n","      print(\"The number is not armstrong.\")\n","\n","\n","num = Mynumber()\n","num.assign()\n","num.FindArmstrong()\n","\n"],"metadata":{"id":"MCxA1C9Oq4Vs","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1653183237827,"user_tz":300,"elapsed":6555,"user":{"displayName":"Avi Paleti","userId":"12042715285535176037"}},"outputId":"90bc8d3d-af11-4bc8-c159-866feda48be6"},"execution_count":12,"outputs":[{"output_type":"stream","name":"stdout","text":["Enter the units digit: 3\n","Enter the tens digit: 5\n","Enter the hundreds digit: 1\n","The number is armstrong.\n"]}]},{"cell_type":"code","source":["#QUESTION 7\n","\n"],"metadata":{"id":"6WbIuUql8M46"},"execution_count":null,"outputs":[]}]}